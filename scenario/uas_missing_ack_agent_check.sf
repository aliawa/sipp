<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">

<scenario name="UAS handling ACK timeout">
	
   <recv request="INVITE" crlf="true" rrs="true" next="2" test="6">
     <action>
		 <ereg regexp="sip:[[:alnum:]]*@[0-9.]*(:[0-9]*)?" search_in="hdr" header="From:" assign_to="1" />
		 <ereg regexp="sip:[[:alnum:]]*@[0-9.]*(:[0-9]*)?" search_in="hdr" header="To:" assign_to="2" />
		 <ereg regexp="sip:[[:alnum:]]*@([0-9.]*(:[0-9]*)?)" search_in="msg" assign_to="4,5" />
		 <ereg regexp="tag=.*" search_in="hdr" header="From:" assign_to="3" />
         <ereg regexp="ewb2bua" search_in="hdr" header="User-Agent:" assign_to="6"/>
     </action>
   </recv>

   <Reference variables="4" />

   <!-- if the User-Agent in INVITE is not B2BUA, we land here -->
   <send>
     <![CDATA[

       SIP/2.0 403 Forbidden
       [last_Via:]
       [last_From:]
       [last_To:]
       [last_Call-ID:]
       [last_CSeq:]
       Content-Length: 0

     ]]>
   </send>

   <recv crlf="true" request="ACK" next="19"/>



   <!-- Jump here if the user-agent check in INVITE is successful -->
   <label id="2"/>



   
   <send>
    <![CDATA[

      SIP/2.0 180 Ringing
      [last_Via:]
      [last_From:]
      [last_To:];tag=[pid]SIPpTag01[call_number]
      [last_Call-ID:]
      [last_CSeq:]
      Contact: <sip:[local_ip]:[local_port];transport=[transport]>
      Content-Length: 0

    ]]>
  </send>

  <send retrans="500" ontimeout="9">
    <![CDATA[

      SIP/2.0 200 OK
      [last_Via:]
      [last_From:]
      [last_To:];tag=[pid]SIPpTag01[call_number]
      [last_Call-ID:]
      [last_CSeq:]
      Contact: <sip:[local_ip]:[local_port];transport=[transport]>
      Content-Type: application/sdp
      Content-Length: [len]

      v=0
      o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
      s=-
      c=IN IP[media_ip_type] [media_ip]
      t=0 0
      m=audio [media_port] RTP/AVP 0
      a=rtpmap:0 PCMU/8000

    ]]>
  </send>

  <recv request="ACK"
	  optional="true"
      crlf="true" >
  </recv>
  
  
  <recv request="BYE">
  </recv>

  <send next="19" crlf="true" >
    <![CDATA[

      SIP/2.0 200 OK
      [last_Via:]
      [last_From:]
      [last_To:]
      [last_Call-ID:]
      [last_CSeq:]
      Contact: <sip:[local_ip]:[local_port];transport=[transport]>
      Content-Length: 0

    ]]>
  </send>

  <label id="9"/>

  <send retrans="500" >
    <![CDATA[

      BYE [$1] SIP/2.0
      Via: SIP/2.0/UDP [$5];branch=z9hG4bk-[pid][call_number]
      From: <[$2]>;tag=[pid]SIPpTag01[call_number] 
      To: <[$1]>;[$3]
      [last_Call-ID:]
      CSeq: 2 BYE
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <recv response="200" crlf="true">
  </recv>

  <label id="19"/>
  
  <!-- Keep the call open for a while in case the 200 is lost to be     -->
  <!-- able to retransmit it if we receive the BYE again.               -->
  <pause milliseconds="4000"/>


  <!-- definition of the response time repartition table (unit is ms)   -->
  <ResponseTimeRepartition value="10, 20, 30, 40, 50, 100, 150, 200"/>

  <!-- definition of the call length repartition table (unit is ms)     -->
  <CallLengthRepartition value="10, 50, 100, 500, 1000, 5000, 10000"/>

</scenario>

