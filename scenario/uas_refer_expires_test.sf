<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">


<scenario name="UAS REFER">
    
  <recv request="INVITE" next="2" test="1">
    <action>
      <!-- Make sure the invite came from B2BUA -->
      <ereg regexp="ewb2bua" search_in="hdr" header="User-Agent:" assign_to="1"/>
      <!-- Save the From tag -->
      <ereg regexp="tag=.*" search_in="hdr" header="From:" assign_to="2" />
      <!-- Extract Contact URI -->
      <ereg regexp="sip:.*([0-9]{1,3}\.){3}[0-9]{1,3}(:[0-9]*)?" search_in="hdr" header="Contact:" assign_to="3" />
      <!-- Logging for debug purposes -->
      <log message="found text [$2]" />
    </action>
  </recv>

  
  <!-- If the user-agent is not B2BUA we land here -->
  <send>
    <![CDATA[

      SIP/2.0 403 Forbidden
      [last_Via:]
      [last_From:]
      [last_To:]
      [last_Call-ID:]
      [last_CSeq:]
      Content-Length: 0

    ]]>
  </send>

  <recv crlf="true" request="ACK" next="19"/>



  <!-- Jump here if the User agnet check in INVITE is successful -->
  <label id="2"/>
  <send>
    <![CDATA[

      SIP/2.0 180 Ringing
      [last_Via:]
      [last_From:]
      [last_To:];tag=[pid]SIPpTag01[call_number]
      [last_Call-ID:]
      [last_CSeq:]
      Contact: <sip:[local_ip]:[local_port];transport=[transport]>
      Content-Length: 0

    ]]>
  </send>

  <send retrans="500">
    <![CDATA[

      SIP/2.0 200 OK
      [last_Via:]
      [last_From:]
      [last_To:];tag=[pid]SIPpTag01[call_number]
      [last_Call-ID:]
      [last_CSeq:]
      Contact: <sip:[local_ip]:[local_port];transport=[transport]>
      Content-Type: application/sdp
      Content-Length: [len]

      v=0
      o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
      s=-
      c=IN IP[media_ip_type] [media_ip]
      t=0 0
      m=audio [media_port] RTP/AVP 0
      a=rtpmap:0 PCMU/8000
      x-my=0

    ]]>
  </send>

  <recv request="ACK"
        optional="true"
        rtd="true"
        crlf="true">
  </recv>

  <recv request="REFER">
  </recv>

  <send >
    <![CDATA[

      SIP/2.0 202 OK
      [last_Via:]
      [last_From:]
      [last_To:];tag=[pid]SIPpTag01[call_number]
      [last_Call-ID:]
      [last_CSeq:]
      Contact: <sip:[local_ip]:[local_port];transport=[transport]>
      Content-Length: [len]

    ]]>
  </send>

  <send retrans="500">
    <![CDATA[

      NOTIFY [$3] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: [field1] <sip:[field1]@[local_ip]:[local_port]>;tag=[pid]-sipp-01
      To:  <sip:[field0]@[local_ip]>;[$2]
      Call-ID: [call_id]
      CSeq: 2 NOTIFY
      Contact: sip:[field1]@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Event: refer
      Subscription-State: active;expires=30
      Content-Type: message/sipfrag
      Content-Length: [len]

      SIP/2.0 100 Trying

    ]]>
  </send>

  <recv response="200">
  </recv>





  <recv request="BYE">
  </recv>

  <send>
    <![CDATA[

      SIP/2.0 200 OK
      [last_Via:]
      [last_From:]
      [last_To:]
      [last_Call-ID:]
      [last_CSeq:]
      Contact: <sip:[local_ip]:[local_port];transport=[transport]>
      Content-Length: 0

    ]]>
  </send>

  <label id="19"/>

</scenario>

